{
  "name": "space",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.10.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^11.4.1",
    "ace-builds": "^1.4.11",
    "nanoid": "^3.1.9",
    "polished": "^3.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-scripts": "3.4.0",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "netlify deploy --dir=build --prod",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm run lint-css && npm run lint-js",
    "lint:fix": "npm run lint-css:fix && npm run lint-js:fix",
    "lint-js": "eslint src --ext .js,.ts,.tsx --max-warnings=0",
    "lint-js:fix": "eslint src --ext .js,.ts,.tsx --fix",
    "lint-css": "stylelint \"src/**/*.js\" --syntax css-in-js",
    "lint-css:fix": "stylelint \"src/**/*.js\" --syntax css-in-js --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "^10.2.9",
    "netlify-cli": "^2.53.0",
    "prettier": "^2.0.5",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/src/*",
      "!<rootDir>/src/**/Styled*"
    ]
  }
}
